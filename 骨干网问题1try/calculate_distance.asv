function [dis]=calculate_distance()
city_x=[474 455 532 450 449 486 380 506 571 609 326 511 646 314 119 181 726 844]
city_y=[322 334 331 391 251 408 322 433 400 334 349 113 240 488 566 501 362 264]
dis=zeros(city_x,city_y)
for i = 1:size(city_x)
    for j = size(city_y)
        dis(i,j)=((city_x(i)-city_x(j))^2+(city_y(i)-city_y(j))^2)^(1/2)*0.85079834
        if(dis(i,j)>=200 || dis(i,j)==0)
            dis(i,j)=-1;
        end
    end
end